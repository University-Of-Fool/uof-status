<!DOCTYPE html>
<html>
<%- include("_particials/head/head.ejs"); %>

<body>
  <div class="container pt-3 gap-3">
    <div class="row pb-3">
      <div class="col"><span class="h1">University of Fool: Status</span></div>
    </div>
    <div class="row">
      <div class="col">
        <div class="mdc-card mdc-card--outlined" style="padding: 16px;">
          <span class="h3">服务器状态</span>
          <div class="mdc-data-table">
            <div class="mdc-data-table__table-container">
              <table class="mdc-data-table__table" aria-label="Server Status">
                <thead>
                  <tr class="mdc-data-table__header-row">
                    <th class="mdc-data-table__header-cell" role="columnheader" scope="col">服务器</th>
                    <th class="mdc-data-table__header-cell " role="columnheader" scope="col">状态</th>
                    <th class="mdc-data-table__header-cell mdc-data-table__header-cell--numeric" role="columnheader" scope="col">负载</th>
                    <th class="mdc-data-table__header-cell" role="columnheader" scope="col">服务</th>
                  </tr>
                </thead>
                <tbody class="mdc-data-table__content">
                  <% statuses.forEach((value,index)=>{ %>
                    <tr class="mdc-data-table__row">
                        <th class="mdc-data-table__cell" scope="row">Frozen yogurt</th>
                        <td class="mdc-data-table__cell mdc-data-table__cell--numeric">24</td>
                        <td class="mdc-data-table__cell mdc-data-table__cell--numeric">4.0</td>
                        <td class="mdc-data-table__cell">Super tasty</td>
                    </tr>
                  <% }); %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <%- include("_particials/foot/footer.ejs"); %>
  <script>

  </script>
</body>

</html>